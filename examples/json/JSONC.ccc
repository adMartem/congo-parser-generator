/*
 * A JSONC (JSON with comments) grammar 
 */
PARSER_PACKAGE=org.parsers.jsonc;
PARSER_CLASS=JSONCParser;
DEFAULT_LEXICAL_STATE=JSON;
USES_PREPROCESSOR;

#if localtest
X_SYNTHETIC_NODES_ENABLED;
#endif

UNPARSED #Comment :
    <SINGLE_LINE_COMMENT : "//" (~["\n"])* >
    |
    //< MULTI_LINE_COMMENT : "/*" (~["*"])* "*" (~["*","/"] (~["*"])* "*" | "*")* "/">
    < ?MULTI_LINE_COMMENT : "/*" (~[])* "*/">
;

INCLUDE "/include/json/JSON.ccc"
